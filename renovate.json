{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "schedule:monthly",
    ":automergePatch",
    ":automergeDigest",
    ":automergeMinor",
    ":automergePr",
    ":label(renovate)",
    ":dependencyDashboard",
    "group:allNonMajor",
    "group:allDigest"
  ],
  "vulnerabilityAlerts": {
    "labels": [
      "security"
    ],
    "automerge": true
  }
  ,
  "packageRules": [
    {
      "matchDatasources": ["git"],
      "matchPackagePatterns": ["^https://github.com/psytraxx/"],
      "enabled": false
    }
  ]
}
